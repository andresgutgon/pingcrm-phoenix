{
  "name": "pingcrm-phoenix-react",
  "author": "Andrés Gutiérrez",
  "version": "1.0.0",
  "private": true,
  "description": "PingCRM inertia.js example with Phoenix and TailwindCSS",
  "keywords": [
    "phoenix",
    "elixir",
    "tailwindcss",
    "typescript",
    "vite"
  ],
  "scripts": {
    "dev": "NODE_ENV=development vite --host 0.0.0.0 --mode development",
    "js:client:prod": "NODE_ENV=production vite build --mode production",
    "js:server:prod": "NODE_ENV=production VITE_SSR=true vite build --mode production",
    "build": "pnpm run js:client:prod && pnpm run js:server:prod",
    "tc": "tsc --noEmit -p tsconfig.app.json",
    "lint": "eslint js/",
    "prettier": "prettier --write \"js/**/*.{ts,tsx}\" --ignore-path .prettierignore",
    "prettier:check": "prettier --check \"js/**/*.{ts,tsx}\" --ignore-path .prettierignore"
  },
  "packageManager": "pnpm@9.12.2",
  "type": "module",
  "dependencies": {
    "@inertiajs/core": "^2.1.5",
    "@inertiajs/react": "^2.1.5",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "axios": "1.11.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.501.0",
    "next-themes": "^0.4.6",
    "phoenix": "^1.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.17",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.17",
    "@types/phoenix": "^1.6.5",
    "@types/react": "19",
    "@types/react-dom": "19",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.20",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "postcss": "8.4.41",
    "prettier": "3.3.3",
    "tailwindcss": "4.0.17",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.11.0",
    "vite": "^6.2.0",
    "vitest": "^2.1.4"
  }
}
